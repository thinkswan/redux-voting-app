{
  "name": "redux-voting-app",
  "version": "0.0.0",
  "description": "A React app that allows users to vote for things and uses Redux.",
  "author": "Graham Swan <thinkswan@gmail.com>",
  "main": "index.js",
  "scripts": {
    "server": "babel-node index.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thinkswan/redux-voting-app.git"
  },
  "bugs": {},
  "keywords": [
    "node",
    "socket-io",
    "babel",
    "immutable",
    "chai",
    "mocha",
    "redux"
  ],
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.5.0",
    "chai-immutable": "^1.5.4",
    "mocha": "^2.4.5"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "dependencies": {
    "concurrently": "^4.1.1",
    "immutable": "^3.7.6",
    "redux": "^3.3.1",
    "socket.io": "^1.4.5"
  },
  "private": true,
  "license": "MIT"
